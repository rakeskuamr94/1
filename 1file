#include <iostream>
#include <algorithm>

void reverseArray(int arr[], int size) {
    std::reverse(arr, arr + size);
}

int main() {
    int numbers[] = {1, 2, 3, 4, 5};
    int size = sizeof(numbers) / sizeof(numbers[0]);
    reverseArray(numbers, size);

    for (int i = 0; i < size; i++) {
        std::cout << numbers[i] << " ";
    }

    std::cout << std::endl;

    return 0;
}
